{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1",
    "^src/(.*)\\.js$": "<rootDir>/../src/$1",
    "^src/(.*)$": "<rootDir>/../src/$1"
  },
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!uuid|@supabase)/"
  ]
}
